{
    "name": "stillat/statamic-search-report",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stillat\\StatamicSearchReport\\": "src"
        }
    },
    "require": {
        "php": "^8.0",
        "statamic/cms": "^4.0 || ^5 || ^6",
        "stillat/statamic-template-resolver": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0 || ^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Stillat\\StatamicSearchReport\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Search Report for Statamic",
            "description": "Provides a simple report of search queries performed on your Statamic site."
        },
        "laravel": {
            "providers": [
                "Stillat\\StatamicSearchReport\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true,
            "pestphp/pest-plugin": true
        }
    }
}
